<%= form_tag(contact_us_path, method: 'post', multipart: true, id: "contactform") do %>
    <p><%= text_field_tag 'name', nil, placeholder: 'Ditt namn *' %></p>
    <p><%= email_field_tag 'email', nil, placeholder: 'Din e-post *' %></p>
    <p><%= text_area_tag 'message', nil, placeholder: 'Ditt meddelande', rows: 5 %></p>
    <p><%= button_tag(type: 'submit') do %>
    Skicka meddelande
    <% end %></p>
<% end %>
